<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.boogipop.memshell.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InjectFilter</servlet-name>
    <servlet-class>com.boogipop.memshell.AddFilterShell</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InjectServlet</servlet-name>
    <servlet-class>com.boogipop.memshell.AddServletShell</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InjectFilter</servlet-name>
    <url-pattern>/inject1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InjectServlet</servlet-name>
    <url-pattern>/inject2</url-pattern>
  </servlet-mapping>
</web-app>